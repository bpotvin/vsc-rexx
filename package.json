{
    "name": "rexxtools",
    "displayName": "rexx",
    "description": "rexx language support",
    "version": "0.23.6",
    "preview": false,
	"icon": "rexx.png",
    "publisher": "cfsops",
    "readme": "README.md",
    "author": {
        "name": "Bill Potvin"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "keywords": [
        "Rexx"
    ],
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "rexx",
            "aliases": ["Rexx", "rexx"],
            "extensions": [".rex"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "rexx",
            "scopeName": "source.rexx",
            "path": "./syntaxes/rexx.tmLanguage"
        }],
        "snippets": [{
            "language": "rexx",
            "path": "./snippets/rexx.json"
        }]
    },
	"repository": {
		"url": "https://github.com/bpotvin/vsc-rexx.git",
		"type": "git"
	}
}