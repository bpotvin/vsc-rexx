{
    "name": "rexxtools",
    "displayName": "rexx",
    "description": "rexx language support",
    "version": "0.0.3",
    "publisher": "Bill Potvin",
    "readme": "README.md",
    "author": {
        "name": "Bill Potvin"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "keywords": [
        "Rexx"
    ],
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:rexx"
    ],
    "contributes": {
        "languages": [{
            "id": "rexx",
            "aliases": ["Rexx", "rexx"],
            "extensions": [".rex"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "rexx",
            "scopeName": "source.rexx",
            "path": "./syntaxes/rexx.tmLanguage.json"
        }]
    }
}